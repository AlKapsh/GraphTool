{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    
    "rootDir": "src",
    "outDir": "dist",
    
    "baseUrl": "src",
    "paths": {
      "Models/*" : ["Models/*"],
      "Services/*" : ["Services/*"],
      "Utils/*" : ["Utils/*"],
    }

  },
  "include": ["src"]
}
